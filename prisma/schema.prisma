generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URI")
}

model User {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  age Int
  post Post[]
}

model Post {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  userId String @db.ObjectId
  user User @relation(fields: [userId], references: [id])
}

model Reviews {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
}